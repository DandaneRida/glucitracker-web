{
  "version": 2,
  "public": true,
  "buildCommand": "echo 'Static build'",
  "outputDirectory": ".",
  "env": {},
  "regions": ["cdg1"],
  "routes": [
    {
      "src": "^/api/(.*)",
      "dest": "/dev-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "dev-server.js": {
      "runtime": "nodejs16.x"
    }
  }
}
