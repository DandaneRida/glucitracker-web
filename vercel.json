{
  "version": 2,
  "public": true,
  "buildCommand": "echo 'Static build'",
  "outputDirectory": ".",
  "env": {},
  "regions": ["cdg1"],
  "routes": [
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/data/(.*)",
      "dest": "/data/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
